  <rdf:Description rdf:about="https://standards.isotc211.org/19157/-3/1/dqc/content/formulaType/FT28_4">
    <dqm:formulaKey>Formula for calculating the mean positional uncertainties of the horizontal absolute or external positions.</dqm:formulaKey>
    <dqm:formulaLanguageVersion>1.0</dqm:formulaLanguageVersion>
