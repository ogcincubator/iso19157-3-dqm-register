  <rdf:Description rdf:about="https://standards.isotc211.org/19157/-3/1/dqc/content/measureDescription/MD10e">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <foaf:depiction rdf:resource="https://ogcincubator.github.io/iso19157-3-dqm-register/ISO19157-3/img/10e_3.PNG"/>
