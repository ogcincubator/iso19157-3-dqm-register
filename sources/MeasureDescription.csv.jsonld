    {
      "measureDescriptionID": "MD 15",
      "sequence": null,
      "extendedDescription": [],
      "formula": [],
      "textDescription": "True indicates that all items is in conformance with its value domain.",
      "prefLabel": "MD 15",
      "@type": "skos:Concept"
    },
    {
      "measureDescriptionID": "MD15e",
      "sequence": null,
      "extendedDescription": [],
      "formula": [],
      "textDescription": "All dates are correct according to their month rules of probable number of days, e.g.  No 29 February in a non-leap year, no 31 April 31 are allowed values.",
      "prefLabel": "MD15e",
      "@type": "skos:Concept"
    },
    {
      "measureDescriptionID": "MD16",
      "sequence": null,
      "extendedDescription": [],
      "formula": [],
      "textDescription": "Conceptual schema describes valid values in the domain.",
      "prefLabel": "MD16",
