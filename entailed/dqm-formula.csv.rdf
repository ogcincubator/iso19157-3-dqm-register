  <rdf:Description rdf:about="https://standards.isotc211.org/19157/-3/1/dqc/content/formulaType/FT49_4">
    <skos:definition>&lt;math xmlns="http://www.w3.org/1998/Math?MathML"&gt;&lt;mo&gt;=&lt;/mo&gt;&lt;mfrac bevelled="true"&gt;&lt;mfenced close="|" open="|"&gt;&lt;msub&gt;&lt;mi&gt;μ&lt;/mi&gt;&lt;mi&gt;H&lt;/mi&gt;&lt;/msub&gt;&lt;/mfenced&gt;&lt;msub&gt;&lt;mi&gt;σ&lt;/mi&gt;&lt;mi&gt;H&lt;/mi&gt;&lt;/msub&gt;&lt;/mfrac&gt;&lt;/math&gt;</skos:definition>
