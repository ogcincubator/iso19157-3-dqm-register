    dqm:formulaLanguage "MathML" ;
    dqm:formulaLanguageVersion "1.0" ;
    skos:inScheme <https://standards.isotc211.org/19157/-3/1/dqc/content/formulaType> .

<https://standards.isotc211.org/19157/-3/1/dqc/content/formulaType/FT49_6> a skos:Concept ;
    dqm:formulaKey "then" ;
    dqm:formulaLanguage "MathML" ;
    dqm:formulaLanguageVersion "1.0" ;
    skos:inScheme <https://standards.isotc211.org/19157/-3/1/dqc/content/formulaType> .

<https://standards.isotc211.org/19157/-3/1/dqc/content/formulaType/FT49_7> a skos:Concept ;
    dqm:formulaKey "If ratio" ;
    dqm:formulaLanguage "MathML" ;
    dqm:formulaLanguageVersion "1.0" ;
    skos:inScheme <https://standards.isotc211.org/19157/-3/1/dqc/content/formulaType> .

<https://standards.isotc211.org/19157/-3/1/dqc/content/formulaType/FT49_8> a skos:Concept ;
    dqm:formulaKey "then calculate k, the ratio of the mean to the standard deviation, using a cubic polynomial fit through the tabular values as defined in the CRC Handbook of Tables for Probability and Statistics (ISBN 13: 9780849306921)" ;
